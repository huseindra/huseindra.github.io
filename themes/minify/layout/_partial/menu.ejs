
<!-- Begin Nav
================================================== -->
<nav class="navbar navbar-toggleable-md navbar-light bg-white fixed-top mediumnavigation post-mediumnavigation">
<button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
<span class="navbar-toggler-icon"></span>
</button>
<div class="container">

	<!-- Begin Logo -->
	<a class="navbar-brand" href="/">
	<img src="<%- config.root %><%- theme.default_logo %>" alt="logo">
	</a>
	<!-- End Logo -->
	<div class="collapse navbar-collapse" id="navbarsExampleDefault">
		<!-- Begin Menu -->
		<ul class="navbar-nav ml-auto">

				<% for (var i in theme.menu){ %>
                     <li class="nav-item active">
                         <a class="nav-link item-post-word" href="<%- theme.menu[i].url ? url_for(theme.menu[i].url) : url_for(theme.menu[i]) %>">
                             <% if (theme.menu[i].icon) { %>
                                 <i class="fa fa-<%= theme.menu[i].icon %> fa-stack-2x"></i>
                             <% } else { %>
                                 <%= i %>
                             <% } %>
                         </a>
                     </li>
                 <% } %>
								 <%- partial('_partial/partial-menu') %>


		</ul>
		<!-- End Menu -->
	</div>
</div>
</nav>
<!-- End Nav
================================================== -->
